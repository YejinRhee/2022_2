from collections import deque
n_tc = int(input())
for _ in range(n_tc):
    n = int(input())
    init_chs = list(input().split())
    ans = deque([])  
    ans.append(init_chs[0])
    for i in range(1,n):
        if ord(ans[0]) >= ord(init_chs[i]):
            ans.appendleft(init_chs[i])
        else:
            ans.append(init_chs[i])
    print(''.join(ans)) 
            
    
  
    







